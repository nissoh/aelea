{
  "name": "aelea",
  "version": "1.9.0",
  "type": "module",
  "scripts": {
    "build": "bun run build:clean && bun run bunx tsc -b",
    "build:esm": "tsc -b",
    "build:clean": "rm -rf dist",
    "tsc:check": "bun run bunx tsc -p tsconfig.json --noEmit"
  },
  "license": "MIT",
  "author": "Nissan Hanina <nissanhanina@gmail.com>",
  "files": [
    "dist"
  ],
