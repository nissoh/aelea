    "./*": {
      "types": "./dist/types/*/index.d.ts",
      "import": "./dist/esm/*/index.js"
    }
  },
  "dependencies": {
    "@most/core": "1.6.1",
    "@most/disposable": "1.3.0",
    "@most/prelude": "1.8.0",
    "@most/scheduler": "1.3.0",
    "@most/types": "1.1.0",
    "csstype": "3.1.3"
  },
  "devDependencies": {
    "@types/bun": "1.2.19",
    "tinybench": "4.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
