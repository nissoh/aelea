{
  "name": "fufu",
  "version": "0.0.3",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "browser": "dist/browser/index.js",
  "typings": "dist/type-definitions/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "rollup": "./../../node_modules/.bin/rollup -c rollup.config.js --",
    "rollup:watch": "./../../node_modules/.bin/rollup -w -c rollup.config.js",
    "build": "npm run clean && npm run rollup",
    "dev": "npm run rollup:watch",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "./../../node_modules/.bin/typed-test -r jsdom-global/register src/*.test.ts --browser chrome",
    "lint": "./../../node_modules/.bin/tslint --fix 'src/**/*.ts'",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nissoh/fufu"
  },
  "dependencies": {
    "@most/core": "1.6.1",
    "@most/disposable": "1.3.0",
    "@most/prelude": "1.8.0",
    "@most/scheduler": "1.3.0",
    "@most/types": "1.1.0",
    "csstype": "3.0.2"
  },
  "license": "MIT",
  "author": "Nissan Hanina <nissan@yo.ma>",
  "gitHead": "34f90530df65dc490678d3b0e0fb018c294befb6"
}
