{
  "name": "match-trie",
  "version": "0.0.0-alpha.1",
  "main": ".dist/commonjs/index.js",
  "module": ".dist/es6/index.js",
  "typings": ".dist/type-definitions/index.d.ts",
  "scripts": {
    "clean": "rm -rf .dist",        
    "build": "npm run clean && npm run build:commonjs & npm run build:es2015",
    "build:commonjs": "tsc -P tsconfig.json --module commonjs --outDir .dist/commonjs",
    "build:es2015": "tsc -P tsconfig.json --module es6 --outDir .dist/es6 --declaration true  --declarationDir .dist/type-definitions",
    "test": "npm run lint && typed-test src/**/*.test.ts",
    "lint": "tslint --fix 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nissoh/fufu"
  },
  "dependencies": {
    "@most/core": "^1.2.0",
    "@most/disposable": "^1.1.0",
    "@most/prelude": "^1.7.0",
    "@most/scheduler": "^1.1.0",
    "@most/types": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^2.4.2",
    "@typed/test": "^2.5.0",
    "tslint": "^5.7.0"
  },
  "license": "MIT",
  "author": "Nissan Hanina <nissan@yo.ma>"
}
