{
  "compilerOptions": {
    "incremental": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "strict": true,
    // "strictFunctionTypes": false,
    "pretty": true,
    // "keyofStringsOnly": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "declaration": true,
    "lib": [
      "dom",
      "es5",
      "es2015"
    ],
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node"
  }
}